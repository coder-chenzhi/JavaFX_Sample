<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.popup.SelectController">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="298.0" spacing="5.0">
         <children>
            <RadioButton mnemonicParsing="false" text="Student" onAction="#handleButtonAction">
               <toggleGroup>
                  <ToggleGroup fx:id="type" />
               </toggleGroup >
            </RadioButton>
            <RadioButton mnemonicParsing="false" text="Teacher" toggleGroup="$type" onAction="#handleButtonAction"/>
            <RadioButton mnemonicParsing="false" text="Manager" toggleGroup="$type" onAction="#handleButtonAction"/>
         </children>
      </HBox>
   </children>
</AnchorPane>
